programa {
  funcao inicio() {

    inteiro a, b, c, soma

    escreva ("Primeiro Numero: ")
    leia (a)
    escreva ("Segundo Numero ")
    leia (b)
    escreva ("Terceiro Numero: ")
    leia (c)

    soma = a + b

    se (soma < c){
      escreva ("Soma é menor que C")
    }senao{
      escreva ("Soma é maior que C")
    }
    
  }
}
